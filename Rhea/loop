//loop



int apple(int x){

    for(int a = 0 ; a <27 ; a++)
    {

        x = x + 3;//6+3+3+3+3+


    }
    int a  = 5;

    return x;

}


int t = apple(6);




//-------------------------------while-----------------------------------
int c (int x)
{
        while(x < 5)
        {
            x = x + 3;
        }
}
 c(1);      //=7
